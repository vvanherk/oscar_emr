<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.oscarehr.common.model">
	<class name="OscarAppointment" table="appointment">
		<id name="appointment_no" unsaved-value="0">
			<generator class="native" />
		</id>
                <property name="provider_no" />
                <property name="appointment_date" />
                <property name="start_time" />
                <property name="end_time" />
                <property name="name" />
                <property name="demographic_no" />
                <property name="program_id" />
                <property name="notes" />
                <property name="reason" />
                <property name="site" />
                <property name="resources" />
                <property name="type" />
                <property name="style" />
                <property name="billing" />
                <property name="status" />
                <property name="createdatetime" />
                <property name="updatedatetime" />
                <property name="creator" />
                <property name="lastupdateuser" />
                <property name="remarks" />
    </class>
</hibernate-mapping>
